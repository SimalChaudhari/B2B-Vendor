{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
    "rewrites": [
      {
        "source": "/auth/verify-otp",
        "destination": "/api/auth/verify-otp"
      }
    ]
  
  
}
